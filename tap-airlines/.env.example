# Tap Airlines - Aeropuertos Argentina (all-flights)

# Required: API Key for the Aeropuertos Argentina endpoint
TAP_AIRLINES_API_KEY=your_api_key_here

# Required: Base API URL (without /all-flights)
TAP_AIRLINES_API_URL=https://webaa-api-h4d5amdfcze7hthn.a02.azurefd.net/web-prod/v1/api-aa

# Optional: Origin header
TAP_AIRLINES_ORIGIN=https://www.aeropuertosargentina.com

# Required: Airports to query (JSON array)
TAP_AIRLINES_AIRPORTS=["AEP","EZE"]

# Optional: Days back to query (1 = today + yesterday)
TAP_AIRLINES_DAYS_BACK=1

# Optional: User-Agent header
TAP_AIRLINES_USER_AGENT=Mozilla/5.0

# Optional: Accept-Language header
TAP_AIRLINES_LANGUAGE=es-AR
